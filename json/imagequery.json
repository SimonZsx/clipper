{
	"appName": "imagequery",
	"num_containers": 5,
	"supportSwarm": true,
	"images": {
		"bigball": ["imagequery:bigball"],
		"clipper": ["imagequery:clipper"],
		"withoutProxy": [
			"imagequery_wo_proxy:c0", 
			"imagequery_wo_proxy:c1", 
			"imagequery_wo_proxy:c2", 
			"imagequery_wo_proxy:c3",
			"imagequery_wo_proxy:c4"
		],
		"withProxy": [
			"imagequery_w_proxy:c0", 
			"imagequery_w_proxy:c1", 
			"imagequery_w_proxy:c2", 
			"imagequery_w_proxy:c3",
			"imagequery_w_proxy:c4"
		]
	},
	"buildFilePath": {
		"bigball": "/clipper/applications/imagequery_bigball/build.sh",
		"clipper": "/clipper/applications/imagequery_clipper/container/build.sh",
		"withoutProxy": "/clipper/applications/build_imagequery_wo_proxy.sh",
		"withProxy": "/clipper/applications/build_imagequery_w_proxy.sh"
	},
	"frontendServerPath": { 
		"bigball": "/clipper/applications/imagequery_bigball/run.sh",
		"clipper": "#",
		"withoutProxy": "/clipper/applications/run_imagequery_wo_proxy.sh",
		"withProxy": "/clipper/clipper_admin/imagequery.py"
	},
	"frontendServerArgs": { 
		"bigball": "",
		"clipper": "",
		"withoutProxy": "",
		"withProxy": "--dag /clipper/applications/imagequery_w_proxy/dag_formatted"
	},
	"frontendClientPath": {
		"bigball":"",
		"clipper":"",
		"withProxy":"/clipper/clipper_admin/imagequery_concurrent_client.py",
		"withoutProxy":"/clipper/clipper_admin/imagequery_concurrent_client.py"
	},	
	"frontendClientParams": {
		"bigball":{
			"worker": "",
			"system": "",
			"port": "",
			"ip": ""
		},
		"clipper":{
			"worker": "",
			"system": "",
			"port": "",
			"ip": ""
		},
		"withProxy":{
			"worker": "1",
			"system": "oursystem",
			"port": "22223",
			"ip": "172.18.0.15"
		},
		"withoutProxy":{
			"worker": "1",
			"system": "withoutproxy",
			"port": "22222",
			"ip": "172.18.0.15"
		}
	}
}
