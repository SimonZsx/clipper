from datetime import datetime
t1 = datetime.utcnow()
print("\n[INFO]\t", "[c1]\t", str(t1))
t2 = datetime.utcnow()
print("[INFO]\t", "[c1]\t", str(t2))
print("[INFO]\t", "[c1]\t", "Time elapsed: ", (t2-t1).total_seconds(), " seconds." )


